<h1>Listing incomes</h1>

<table>
  <tr>
    <th>Money</th>
    <th>Category</th>
    <th>Description</th>
    <th>Date</th>
  </tr>

<% for income in @incomes %>
  <tr>
    <td><%=h income.money %></td>
    <td><%=h income.category %></td>
    <td><%=h income.description %></td>
    <td><%=h income.date %></td>
    <td><%= link_to 'Show', income %></td>
    <td><%= link_to 'Edit', edit_income_path(income) %></td>
    <td><%= link_to 'Destroy', income, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New income', new_income_path %>
